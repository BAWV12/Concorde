<?xml version="1.0" ?> 

<PropertyList> 
 <path>wiper.ac</path> 

 <animation>
  <type>rotate</type>
  <object-name>Wiper</object-name>
  <property>controls/wiper/motor[0]/position-norm</property>
  <factor>90</factor>
  <center>
