<!-- ======== -->
<!-- LIGHTING -->
<!-- ======== -->

  <animation>
   <name>Panel lighting</name>
   <object-name>MainPanel_1</object-name>
   <object-name>MainPanel_0</object-name>

  <emission>
   <red>0.83</red>
   <green>0.74</green>
   <blue>0.38</blue>
   <factor-prop>systems/lighting/level/panel/main</factor-prop>
  </emission>


  </animation>
 
  <animation>
   <name>Dash lighting</name>
   <object-name>CeilingPanel</object-name>
   <object-name>Left Pedestal</object-name>
   <object-name>Left Top Console</object-name>
   <object-name>Right Pedestal</object-name>
   <object-name>Right Top Console</object-name>
  </animation>

  <animation>
   <name>Cockpit lighting</name>
   <object-name>Left Captain Foot Plate</object-name>
   <object-name>Left Copilot Foot Plate</object-name>
   <object-name>Right Captain Foot Plate</object-name>
   <object-name>Right Copilot Foot Plate</object-name>
  </animation>

  <animation>
   <name>Console lighting</name>
   <object-name>Pedestal</object-name>
  </animation>

  <animation>
   <name>Rack lighting</name>
   <object-name>CeilingEdge</object-name>
   <object-name>Observer Panel</object-name>
   <object-name>Avionics Racks</object-name>
  </animation>

</PropertyList>
