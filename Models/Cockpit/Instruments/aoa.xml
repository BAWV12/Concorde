<?xml version="1.0"?>
<PropertyList>
  <path>../Models/aoa.ac</path>
  <params>
    <alpha-deg>instrumentation/accelerometer-aoa[0]/alpha-deg</alpha-deg>
  </params>
  <animation>
    <type>translate</type>
    <object-name>Accl</object-name>
    <property>/fdm/jsbsim/accelerations/Nz</property>
    <interpolation>
      <entry>
        <ind>3.5</ind>
        <dep>.073</dep>
      </entry>
      <entry>
        <ind>-1</ind>
        <dep>-.073</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>Aoa</object-name>
    <property alias="/params/alpha-deg"/>
    <interpolation>
      <entry>
        <ind>25</ind>
        <dep>.076</dep>
      </entry>
      <entry>
        <ind>-5</ind>
        <dep>-.076</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
    <emission>
      <red>0.2</red>
      <green>0.1</green>
      <blue>0.1</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Accl</object-name>
    <object-name>Aoa</object-name>
    <emission>
      <red>0.1</red>
      <green>0.03</green>
      <blue>0.03</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
