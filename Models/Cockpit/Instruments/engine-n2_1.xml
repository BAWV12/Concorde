<?xml version="1.0"?>
<PropertyList>
  <path>../Models/enginen2_1.ac</path>
  <params>
    <n2>/engines/engine[0]/n2</n2>
  </params>
  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property alias="/params/n2"/>
    <factor>2.65</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <!--Numbers-->
  <animation>
    <type>rotate</type>
    <object-name>Num1</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <property alias="/params/n2"/>
              <value>100</value>
            </div>
          </floor>
          <max>
            <difference>
              <property alias="/params/n2"/>
              <value>99</value>
              <prod>
                <floor>
                  <div>
                    <property alias="/params/n2"/>
                    <value>100</value>
                  </div>
                </floor>
                <value>100</value>
              </prod>
            </difference>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.00720</x-m>
      <y-m>-0.01400</y-m>
      <z-m>0.01200</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Num2</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <property alias="/params/n2"/>
              <value>10</value>
            </div>
          </floor>
          <max>
            <difference>
              <property alias="/params/n2"/>
              <value>9</value>
              <prod>
                <floor>
                  <div>
                    <property alias="/params/n2"/>
                    <value>10</value>
                  </div>
                </floor>
                <value>10</value>
              </prod>
            </difference>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.00720</x-m>
      <y-m>-0.009</y-m>
      <z-m>0.01200</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Num3</object-name>
    <property alias="/params/n2"/>
    <factor>36</factor>
    <center>
      <x-m>-0.00720</x-m>
      <y-m>-0.004</y-m>
      <z-m>0.01200</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
    <object-name>Needle</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
  </and>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
  </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <!--
  <animation>
    <type>material</type>
    <object-name>VOR1Needle</object-name>
    <object-name>VOR2Needle</object-name>
    <emission>
      <red>0.1</red>
      <green>0.03</green>
      <blue>0.03</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  -->
</PropertyList>
