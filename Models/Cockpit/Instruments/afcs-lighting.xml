<?xml version="1.0"?>
<PropertyList>
  <path>../Models/afcslighting.ac</path>
      <animation>
    <type>pick</type>
    <object-name>Knob</object-name>
    <object-name>Knob1</object-name>
    <action>
      <name>Toggle</name>
      <button>0</button>
      <repeatable>false</repeatable>
     <binding>
    <command>property-toggle</command>
    <property>/controls/lighting/crew/afcs/low</property>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
      </action>
    </animation>

  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
  <condition>
	<greater-than>
	  <property>systems/lighting/level/roof</property>
	  <value>0</value>
	</greater-than>
  </condition>
  <emission>
   <red>0.66</red>
   <green>0.68</green>
   <blue>0.64</blue>
   <factor-prop>systems/lighting/level/roof</factor-prop>
  </emission>
 </animation>
</PropertyList>
