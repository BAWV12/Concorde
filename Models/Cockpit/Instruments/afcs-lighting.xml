<?xml version="1.0"?>
<PropertyList>
  <path>../Models/afcslighting.ac</path>
      <animation>
    <type>pick</type>
    <object-name>Knob</object-name>
    <action>
      <name>Up</name>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/controls/lighting/instruments-norm</property>
        <step>0.1</step>
        <min>0.2</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <name>Dwn</name>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>/controls/lighting/instruments-norm</property>
        <step>-0.1</step>
        <min>0.2</min>
        <max>1</max>
        <wrap>false</wrap>
      </binding>
    </action>
    </animation>

  <animation>
    <type>rotate</type>
    <object-name>Knob</object-name>
    <property>/controls/lighting/instruments-norm</property>
    <factor>200</factor>
    <center>
      <x-m>0.01298</x-m>
      <y-m>0</y-m>
      <z-m>-0.00222</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>


  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
  <condition>
	<greater-than>
	  <property>systems/lighting/level/roof</property>
	  <value>0</value>
	</greater-than>
  </condition>
  <emission>
   <red>0.66</red>
   <green>0.68</green>
   <blue>0.64</blue>
   <factor-prop>systems/lighting/level/roof</factor-prop>
  </emission>
 </animation>
</PropertyList>
