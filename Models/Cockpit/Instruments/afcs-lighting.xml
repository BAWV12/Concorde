<?xml version="1.0"?>
<PropertyList>
  <path>../Models/afcslighting.ac</path>
      <animation>
    <type>pick</type>
    <object-name>Knob</object-name>
    <object-name>Knob1</object-name>
    <action>
      <name>Toggle</name>
      <button>0</button>
      <repeatable>false</repeatable>
     <binding>
    <command>property-toggle</command>
    <property>/controls/lighting/crew/afcs/low</property>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
      </action>
    </animation>

  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
