<?xml version="1.0"?>
<PropertyList>
  <path>../Models/enginefuel.ac</path>

  <params>
    <fuelflow>/engines/engine[0]/fuel-flow_pph</fuelflow>
    <bug-select>/controls/engines/engine[0]/bug_pph</bug-select>
  </params>

<effect>
    <inherits-from>Aircraft/Concorde/Models/Effects/interior</inherits-from>
    <object-name>Frame</object-name>
    <object-name>FuelAdjust</object-name>
</effect>

  <animation>
    <type>select</type>
    <object-name>FF</object-name>
    <condition>
        <less-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </less-than>
    </condition>    
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>FuelAdjust</object-name>
    <object-name>FuelAdjust.001</object-name>
    <property alias="/params/bug-select"/>
    <factor>0.1</factor>
    <center>
      <x-m>0.00855</x-m>
      <y-m>0.02084</y-m>
      <z-m>-0.02499</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <condition><not>
        <less-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </less-than>
    </not></condition>

    <type>rotate</type>
    <object-name>Needle</object-name>
    <property alias="/params/fuelflow"/>
    <factor>0.00272155</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>FuelBug</object-name>
    <property alias="/params/bug-select"/>
    <factor>0.00272155</factor>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <!--Bug Numbers-->
  <animation>
    <type>rotate</type>
    <object-name>Set1</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <div>
                <property alias="/params/bug-select"/>
                <value>2204.624</value>
              </div>
              <value>10</value>
            </div>
          </floor>
          <max>
            <prod>
              <difference>
                <div>
                  <property alias="/params/bug-select"/>
                  <value>2204.624</value>
                </div>
                <value>9.9</value>
                <prod>
                  <floor>
                    <div>
                      <div>
                        <property alias="/params/bug-select"/>
                        <value>2204.624</value>
                      </div>
                      <value>10</value>
                    </div>
                  </floor>
                  <value>10</value>
                </prod>
              </difference>
              <value>10</value>
            </prod>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.0023</x-m>
      <y-m>-0.05</y-m>
      <z-m>0.014</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Set2</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <property alias="/params/bug-select"/>
              <value>2204.624</value>
            </div>
          </floor>
          <max>
            <prod>
              <difference>
                <div>
                  <property alias="/params/bug-select"/>
                  <value>2204.624</value>
                </div>
                <value>.9</value>
                <floor>
                  <div>
                    <property alias="/params/bug-select"/>
                    <value>2204.624</value>
                  </div>
                </floor>
              </difference>
              <value>10</value>
            </prod>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.0023</x-m>
      <y-m>0</y-m>
      <z-m>0.014</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>Set3</object-name>
    <property alias="/params/bug-select"/>
    <factor>0.163293151122</factor>
    <center>
      <x-m>-0.0023</x-m>
      <y-m>0.005</y-m>
      <z-m>0.014</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <!--Flow Numbers-->
  <animation>
    <condition><not>
        <less-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </less-than>
    </not></condition>

    <type>rotate</type>
    <object-name>Num1</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <div>
                <property alias="/params/fuelflow"/>
                <value>2204.624</value>
              </div>
              <value>10</value>
            </div>
          </floor>
          <max>
            <prod>
              <difference>
                <div>
                  <property alias="/params/fuelflow"/>
                  <value>2204.624</value>
                </div>
                <value>9.9</value>
                <prod>
                  <floor>
                    <div>
                      <div>
                        <property alias="/params/fuelflow"/>
                        <value>2204.624</value>
                      </div>
                      <value>10</value>
                    </div>
                  </floor>
                  <value>10</value>
                </prod>
              </difference>
              <value>10</value>
            </prod>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.0072</x-m>
      <y-m>-0.05</y-m>
      <z-m>-0.013</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <condition><not>
        <less-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </less-than>
    </not></condition>

    <type>rotate</type>
    <object-name>Num2</object-name>
    <expression>
      <prod>
        <sum>
          <floor>
            <div>
              <property alias="/params/fuelflow"/>
              <value>2204.624</value>
            </div>
          </floor>
          <max>
            <prod>
              <difference>
                <div>
                  <property alias="/params/fuelflow"/>
                  <value>2204.624</value>
                </div>
                <value>.9</value>
                <floor>
                  <div>
                    <property alias="/params/fuelflow"/>
                    <value>2204.624</value>
                  </div>
                </floor>
              </difference>
              <value>10</value>
            </prod>
            <value>0</value>
          </max>
        </sum>
        <value>36</value>
      </prod>
    </expression>
    <center>
      <x-m>-0.0072</x-m>
      <y-m>0</y-m>
      <z-m>-0.013</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <condition><not>
        <less-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </less-than>
    </not></condition>

    <type>rotate</type>
    <object-name>Num3</object-name>
    <property alias="/params/fuelflow"/>
    <factor>0.163293151122</factor>
    <center>
      <x-m>-0.0072</x-m>
      <y-m>0.005</y-m>
      <z-m>-0.013</z-m>
    </center>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <!-- Adjustment wheel -->
  <animation>
    <type>pick</type>
    <object-name>FuelAdjust</object-name>
    <action>
      <name>Fuel Adjust Up</name>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/bug-select"/>
        <step>220.462</step>
        <min>0</min>
        <max>66138.7</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <name>Fuel Adjust Down</name>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property alias="/params/bug-select"/>
        <step>-220.462</step>
        <min>0</min>
        <max>66138.7</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
    <object-name>Needle</object-name>
    <object-name>FuelAdjust.001</object-name>
    <object-name>FuelBug</object-name>
    <object-name>Set1</object-name>
    <object-name>Num1</object-name>
    <object-name>Set2</object-name>
    <object-name>Num2</object-name>
    <object-name>Set3</object-name>
    <object-name>Num3</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
        <greater-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </greater-than>
  </and>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
  <emission>
     <red>0.2</red>
     <green>0.2</green>
     <blue>0.2</blue>
  </emission>
 </animation>

</PropertyList>
