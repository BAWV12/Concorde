<?xml version="1.0"?>
<PropertyList>
  <path>../Models/nosevisor-status.ac</path>
  <animation>
    <type>textranslate</type>
    <object-name>VisorLabel</object-name>
    <property>/instrumentation/nose-visor/pos-norm</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>.33</ind>
        <dep>.66</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>-1</y>
    </axis>
  </animation>
  <animation>
    <type>select</type>
    <object-name>VisorLabelTransit</object-name>
    <condition>
      <and>
        <not-equals>
          <property>/instrumentation/nose-visor/pos-norm</property>
          <value>0</value>
        </not-equals>
        <less-than>
          <property>/instrumentation/nose-visor/pos-norm</property>
          <value>0.33</value>
        </less-than>
      </and>
    </condition>
  </animation>
  <animation>
    <type>textranslate</type>
    <object-name>NoseLabel</object-name>
    <property>/instrumentation/nose-visor/pos-norm</property>
    <interpolation>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>.33</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>.66</ind>
        <dep>.33</dep>
      </entry>
      <entry>
        <ind>1</ind>
        <dep>.66</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>-1</y>
    </axis>
  </animation>
  <animation>
    <type>select</type>
    <object-name>NoseLabelTransit</object-name>
    <condition>
      <and>
        <greater-than>
          <property>/instrumentation/nose-visor/pos-norm</property>
          <value>0.33</value>
        </greater-than>
        <not-equals>
          <property>/instrumentation/nose-visor/pos-norm</property>
          <value>0.67</value>
        </not-equals>
        <not-equals>
          <property>/instrumentation/nose-visor/pos-norm</property>
          <value>1</value>
        </not-equals>
      </and>
    </condition>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
</PropertyList>
