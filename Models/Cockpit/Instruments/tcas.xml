<?xml version="1.0"?>
<PropertyList>
  <path>../Models/tcas.ac</path>

  <animation>
    <type>select</type>
    <object-name>FF</object-name>
    <condition>
        <less-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </less-than>
    </condition>    
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>VSI</object-name>
    <property>instrumentation/adc[0]/output/vertical-speed-fps</property>
    <center>
      <x-m>0.0</x-m>
      <y-m>0.0</y-m>
      <z-m>-0.0</z-m>
    </center>
     <interpolation>
      <entry><ind>-100.0</ind><dep>-175.0</dep></entry>
      <entry><ind>-80.0</ind><dep>-165.0</dep></entry>
      <entry><ind>-65.0</ind><dep>-150.0</dep></entry>
      <entry><ind>-50.0</ind><dep>-135.0</dep></entry>
      <entry><ind>-33.3</ind><dep>-110.0</dep></entry>
      <entry><ind>-25</ind><dep>-90</dep></entry>
      <entry><ind>-16.7</ind><dep>-65</dep></entry>
      <entry><ind> 0</ind><dep>  0.0</dep></entry>
      <entry><ind>16.7</ind><dep>65</dep></entry>
      <entry><ind>25</ind><dep>90</dep></entry>
      <entry><ind>33.3</ind><dep>110</dep></entry>
      <entry><ind>50.0</ind><dep>135.0</dep></entry>
      <entry><ind>65.0</ind><dep>150.0</dep></entry>
      <entry><ind>80.0</ind><dep>165.0</dep></entry>
      <entry><ind>100.0</ind><dep>180.0</dep></entry>
     </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>


  <!--Default Lighting-->


 <animation>
  <type>material</type>
  <object-name>Adjust.002</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
        <greater-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </greater-than>
  </and>
  </condition>

  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
  </emission>
 </animation>


  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Adjust</object-name>
    <property>instrumentation/efis/inputs/range-nm</property>
    <factor>5</factor>
    <center>
      <x-m>0.1</x-m>
      <y-m>0.02693</y-m>
      <z-m>-0.02676</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
  <animation>
    <type>pick</type>
    <object-name>Adjust</object-name>
    <action>
      <name>Up</name>
      <button>3</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/efis/inputs/range-nm</property>
        <step>5</step>
        <min>5</min>
        <max>40</max>
        <wrap>false</wrap>
      </binding>
    </action>
    <action>
      <name>Dwn</name>
      <button>4</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-adjust</command>
        <property>instrumentation/efis/inputs/range-nm</property>
        <step>-5</step>
        <min>5</min>
        <max>40</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>




</PropertyList>
