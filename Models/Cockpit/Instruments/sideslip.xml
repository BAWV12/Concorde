<?xml version="1.0"?>
<PropertyList>
  <path>../Models/sideslip.ac</path>
  <params>
    <serviceable>/instrumentation/side-slip[0]/serviceable</serviceable>
    <angle-sel>/instrumentation/side-slip[0]/angle-deg</angle-sel>
  </params>
  <animation>
    <type>translate</type>
    <object-name>Slip</object-name>
    <property alias="/params/angle-sel"/>
    <interpolation>
      <entry>
        <ind>15</ind>
        <dep>.047</dep>
      </entry>
      <entry>
        <ind>5</ind>
        <dep>.023</dep>
      </entry>
      <entry>
        <ind>0</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>-5</ind>
        <dep>-.023</dep>
      </entry>
      <entry>
        <ind>-15</ind>
        <dep>-.046</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
    </axis>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
  </and>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
  </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Slip</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
  </and>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
  </emission>
  </animation>
</PropertyList>
