<?xml version="1.0"?>
<PropertyList>
  <path>../Models/cog.ac</path>
  <animation>
    <type>translate</type>
    <object-name>CG</object-name>
    <property>/instrumentation/cg[0]/percent</property>
    <interpolation>
      <entry>
        <ind>50</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>61</ind>
        <dep>-.0735</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>FWD</object-name>
    <property>/instrumentation/cg[0]/min-percent</property>
    <interpolation>
      <entry>
        <ind>50</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>61</ind>
        <dep>-.0735</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>translate</type>
    <object-name>AFT</object-name>
    <property>/instrumentation/cg[0]/max-percent</property>
    <interpolation>
      <entry>
        <ind>50</ind>
        <dep>0</dep>
      </entry>
      <entry>
        <ind>61</ind>
        <dep>-.0735</dep>
      </entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
    </axis>
  </animation>
  <animation>
    <type>select</type>
    <object-name>AlarmOn</object-name>
    <condition>
      <or>
        <greater-than>
          <property>/instrumentation/cg[0]/percent</property>
          <property>/instrumentation/cg[0]/max-percent</property>
        </greater-than>
        <less-than>
          <property>/instrumentation/cg[0]/percent</property>
          <property>/instrumentation/cg[0]/min-percent</property>
        </less-than>
      </or>
    </condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>AlarmOff</object-name>
    <condition>
      <not>
        <or>
          <greater-than>
            <property>/instrumentation/cg[0]/percent</property>
            <property>/instrumentation/cg[0]/max-percent</property>
          </greater-than>
          <less-than>
            <property>/instrumentation/cg[0]/percent</property>
            <property>/instrumentation/cg[0]/min-percent</property>
          </less-than>
        </or>
      </not>
    </condition>
  </animation>
  <!--Default Lighting-->
  <animation>
    <type>material</type>
    <object-name>Guage</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
  </and>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
  </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>Frame</object-name>
    <emission>
      <red>0.01</red>
      <green>0.005</green>
      <blue>0.005</blue>
      <factor-prop>controls/lighting/panel-norm</factor-prop>
    </emission>
  </animation>
  <animation>
    <type>material</type>
    <object-name>CG</object-name>
    <object-name>FWD</object-name>
    <object-name>AFT</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
  </and>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
  </emission>
  </animation>
</PropertyList>
