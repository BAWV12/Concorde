<?xml version="1.0"?>
<PropertyList>
  <path>../Models/ins-label.ac</path>
  <animation>
    <type>material</type>
    <object-name>Label</object-name>
  <condition>
  <and>
   <greater-than>
    <property>sim/time/sun-angle-rad</property>
    <value>1.59</value>
   </greater-than>
    <property>controls/lighting/instrument-lights</property>
  </and>
        <greater-than>
          <property>/systems/electrical/outputs/specific</property>
          <value>20.0</value>
        </greater-than>
  </condition>
  <emission>
   <red>0.7</red>
   <green>0.2</green>
   <blue>0.2</blue>
   <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
  </animation>
</PropertyList>
