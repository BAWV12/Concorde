<PropertyList>

<path>pedals.ac</path>


<!-- PEDALS -->

<animation>
 <type>translate</type>
 <object-name>LHPedal</object-name>
 <property>controls/flight/rudder</property>
 <factor>0.08</factor>
 <axis>
  <x>1.0</x>
  <y>0.0</y>
  <z>0.0</z>
 </axis>
</animation>

<animation>
 <type>translate</type>
 <object-name>RHPedal</object-name>
 <property>controls/flight/rudder</property>
 <factor>-0.08</factor>
 <axis>
  <x>1.0</x>
  <y>0.0</y>
  <z>0.0</z>
 </axis>
</animation>

<animation>
 <type>rotate</type>
 <object-name>LHPedal</object-name>
 <property>controls/gear/brake-left</property>
 <factor>-10</factor>
 <axis>
  <x>0.0</x>
  <y>1.0</y>
  <z>0.0</z>
 </axis>
 <center>
  <x-m>0.0</x-m>
  <y-m>0</y-m>
  <z-m>0.00</z-m>
 </center>
</animation>

<animation>
 <type>rotate</type>
 <object-name>RHPedal</object-name>
 <property>controls/gear/brake-right</property>
 <factor>-10</factor>
 <axis>
  <x>0.0</x>
  <y>1.0</y>
  <z>0.0</z>
 </axis>
 <center>
  <x-m>0.0</x-m>
  <y-m>0</y-m>
  <z-m>0.000</z-m>
 </center>
</animation>
</PropertyList>

