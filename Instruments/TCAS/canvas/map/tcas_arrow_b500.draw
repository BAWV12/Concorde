var draw_tcas_arrow_below_500 = func(group) {

group.createChild("path")
					.moveTo(0,17)
					.vertTo(-17)
					.lineTo(-10,0)
					.moveTo(0,-17)
					.lineTo(10,0)
					.setColor(1,1,1)
					.setTranslation(25,0)
					.setStrokeLineWidth(3);


}