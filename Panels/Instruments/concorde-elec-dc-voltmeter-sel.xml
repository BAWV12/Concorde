  <layer>
   <name>knob rotation</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/navcom-radios.png</path>
    <x1>0.53125</x1>
    <x2>0.59765625</x2>
    <y1>0.05859375</y1>
    <y2>0.1484375</y2>
   </texture>
   <w>55</w>
   <h>60</h>
   <transformations>
    <transformation>
     <type>rotation</type>
     <property>/controls/electric/dc/voltmeter</property>
     <min>-2</min>
     <max>3</max>
     <scale>45</scale>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

  <action>
   <name>selector left</name>
   <button>0</button>
   <x>-43</x>
   <y>-41</y>
   <w>42</w>
   <h>82</h>
   <binding>
    <command>property-adjust</command>
    <property>/controls/electric/dc/voltmeter</property>
    <step>-1</step>
    <min>-2</min>
    <max>3</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>nasal</command>
    <script>globals.Concorde.electricalsystem.dcvoltmeterexport();</script>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
  </action>

  <action>
   <name>selector right</name>
   <button>0</button>
   <x>1</x>
   <y>-41</y>
   <w>42</w>
   <h>82</h>
   <binding>
    <command>property-adjust</command>
    <property>/controls/electric/dc/voltmeter</property>
    <step>1</step>
    <min>-2</min>
    <max>3</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>nasal</command>
    <script>globals.Concorde.electricalsystem.dcvoltmeterexport();</script>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
  </action>

 </actions>

</PropertyList>
