<?xml version="1.0"?>

<!--Concorde speed knob :

    - real range is 130 to 400 kt.
    - the speed dial is dissociated from its autopilot target;
    as speed hold is pressed, the current speed is supposed not echoed on the dial.
-->

<PropertyList>

 <name>Speed setting knob</name>
 <w-base>120</w-base>
 <h-base>120</h-base>


 <layers>


  <layer>
   <name>Title knob</name>
   <type>text</type>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>SPEED SELECT</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-16</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-40</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>


</PropertyList>
