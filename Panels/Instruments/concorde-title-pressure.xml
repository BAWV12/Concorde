<?xml version="1.0"?>

<!-- Cabine pressure block -->

<PropertyList>
 <name>Cabine pressure</name>
 <w-base>280</w-base>
 <h-base>28</h-base>

 <layers>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>CABIN</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>35</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title 2</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>PRESSURE CONTROL</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>y-shift</type>
     <offset>-11</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
