<?xml version="1.0"?>

<!-- Concorde altitude knob :

     - max operating altitude is 60000 ft.
     - the altitude dial is dissociated from its autopilot target;
     as altitude hold is pressed, the current altitude is supposed not echoed on the dial.
-->


<PropertyList>

 <name>Altitude setting knob</name>
 <w-base>120</w-base>
 <h-base>120</h-base>


 <layers>


  <layer>
   <name>Title knob</name>
   <type>text</type>
   <point-size>7</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>ALTITUDE SELECT</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-22</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-40</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 
</PropertyList>
