<?xml version="1.0"?>

<!-- engine starting block -->

<PropertyList>
 <name>Engine starting</name>
 <w-base>280</w-base>
 <h-base>28</h-base>

 <layers>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>10</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>ENGINE STARTING</text>
    </chunk>
   </chunks>
  </layer>

  <layer>
   <name>Subtitle</name>
   <type>text</type>
   <point-size>6</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>START VALVE</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>28</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-10</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

</PropertyList>
