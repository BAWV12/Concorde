<?xml version="1.0"?>

<!-- Concorde AC rotary selector (voltmeter) :

-->

<PropertyList>
 <name>AC rotary selector</name>
 <w-base>205</w-base>
 <h-base>123</h-base>


 <layers>

<!-- SELECTOR LEFT -->

  <layer>
   <name>Title ground power</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>GRND</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-52</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-42</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title ground power (line 2)</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>PWR</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-48</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-58</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title emergency power</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>EMERG</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-91</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>5</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title emergency power (line 2)</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>PWR</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-79</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-12</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title generator 1</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>GEN</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-56</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>32</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title generator 1 (line 2)</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>1</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-45</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>16</offset>
    </transformation>
   </transformations>
  </layer>

<!-- SELECTOR VERTICAL -->

  <layer>
   <name>Title generator 2</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>GEN 2</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-25</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>51</offset>
    </transformation>
   </transformations>
  </layer>

<!-- SELECTOR RIGHT -->

  <layer>
   <name>Title generator 3</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>GEN</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>28</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>32</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title generator 3 (line 2)</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>3</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>39</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>16</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title generator 4</name>
   <type>text</type>
   <point-size>11</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>GEN</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>53</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>5</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>Title generator 4 (line 2)</name>
   <type>text</type>
   <point-size>12</point-size>
   <color>
    <red>1.0</red>
    <green>1.0</green>
    <blue>1.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>4</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>64</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-12</offset>
    </transformation>
   </transformations>
  </layer>

  <layer>
   <name>knob rotation</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/navcom-radios.png</path>
    <x1>0.53125</x1>
    <x2>0.59765625</x2>
    <y1>0.05859375</y1>
    <y2>0.1484375</y2>
   </texture>
   <w>55</w>
   <h>60</h>
   <transformations>
    <transformation>
     <type>rotation</type>
     <property>/controls/electric/ac/voltmeter</property>
     <min>-3</min>
     <max>2</max>
     <scale>45</scale>
     <offset>0</offset>
    </transformation>
   </transformations>
  </layer>

 </layers>

 <actions>

  <action>
   <name>selector left</name>
   <button>0</button>
   <x>-43</x>
   <y>-41</y>
   <w>42</w>
   <h>82</h>
   <binding>
    <command>property-adjust</command>
    <property>/controls/electric/ac/voltmeter</property>
    <step>-1</step>
    <min>-3</min>
    <max>2</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>nasal</command>
    <script>globals.Concorde.electricalsystem.acvoltmeterexport();</script>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
  </action>

  <action>
   <name>selector right</name>
   <button>0</button>
   <x>1</x>
   <y>-41</y>
   <w>42</w>
   <h>82</h>
   <binding>
    <command>property-adjust</command>
    <property>/controls/electric/ac/voltmeter</property>
    <step>1</step>
    <min>-3</min>
    <max>2</max>
    <wrap>0</wrap>
   </binding>
   <binding>
    <command>nasal</command>
    <script>globals.Concorde.electricalsystem.acvoltmeterexport();</script>
   </binding>
   <binding>
    <command>property-toggle</command>
    <property>/instrumentation/generic/click</property>
   </binding>
  </action>

 </actions>

</PropertyList>
