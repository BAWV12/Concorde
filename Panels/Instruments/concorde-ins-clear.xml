<?xml version="1.0"?>

<!-- INS Clear button -->

<PropertyList>
 <name>INS Clear</name>
 <w-base>78</w-base>
 <h-base>26</h-base>


 <layers>

  <layer>
   <name>background</name>
   <texture>
    <path>Aircraft/Concorde/Panels/Instruments/Textures/concorde-white-button.png</path>
   </texture>
   <w>78</w>
   <h>26</h>
  </layer>

  <layer>
   <name>Title</name>
   <type>text</type>
   <point-size>15</point-size>
   <color>
    <red>0.0</red>
    <green>0.0</green>
    <blue>0.0</blue>
   </color>
   <chunks>
    <chunk>
     <type>literal</type>
     <text>CLEAR</text>
    </chunk>
   </chunks>
   <transformations>
    <transformation>
     <type>x-shift</type>
     <offset>-29</offset>
    </transformation>
    <transformation>
     <type>y-shift</type>
     <offset>-3</offset>
    </transformation>
   </transformations>
  </layer>
 </layers>

</PropertyList>
