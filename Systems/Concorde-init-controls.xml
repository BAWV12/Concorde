<?xml version="1.0"?>

<!-- - creates booleans (impossible with Nasal), which would otherwise be empty strings by default.
     - makes more readable the initialization state.

     - see model animation for gear times.


     Known problems :
     ==============
     - /controls/engines/engine[0]/master-bat seems (?) required by FG.
-->

<PropertyList>

  <adc>
   <ivsi-in-cruise type="bool">true</ivsi-in-cruise>          <!-- IVSI instrument above 50000 ft -->
   <ivsi-log type="bool">false</ivsi-log>                     <!-- not log of IVSI changes -->
   <system n="0">
    <ivsi-emulated type="bool">false</ivsi-emulated>          <!-- no IVSI instrument -->
    <num type="int">1</num>
    <selector type="int">0</selector>                         <!-- normal -->
    <switch type="bool">true</switch>
   </system>

   <system n="1">
    <ivsi-emulated type="bool">false</ivsi-emulated>          <!-- no IVSI instrument -->
    <num type="int">2</num>
    <selector type="int">0</selector>                         <!-- normal -->
    <switch type="bool">true</switch>
   </system>
  </adc>

  <anti-ice>
   <engine n="0">
    <inlet-vane type="bool">false</inlet-vane>
   </engine>
   <engine n="1">
    <inlet-vane type="bool">false</inlet-vane>
   </engine>
   <engine n="2">
    <inlet-vane type="bool">false</inlet-vane>
   </engine>
   <engine n="3">
    <inlet-vane type="bool">false</inlet-vane>
   </engine>
   <icing-model>
    <duration>
     <few-min type="int">8</few-min>
     <scattered-min type="int">4</scattered-min>
     <broken-min type="int">2</broken-min>
     <overcast-min type="int">1</overcast-min>
     <clear-min type="int">999</clear-min>                    <!-- icing rain -->
    </duration>
    <temperature>                                             <!-- usually between 0 and -15/-20 -->
     <max-degc type="double">0.0</max-degc>
     <min-degc type="double">-20.0</min-degc>
    </temperature>
   </icing-model>
   <mast>
    <heater n="0" type="bool">true</heater>
    <heater n="1" type="bool">true</heater>
    <heater n="2" type="bool">true</heater>
    <standby n="0" type="bool">false</standby>
    <standby n="1" type="bool">false</standby>
    <standby n="2" type="bool">false</standby>
   </mast>
   <static>
    <heater n="0" type="bool">true</heater>
    <heater n="1" type="bool">true</heater>
   </static>
   <visor>
    <de-ice n="0" type="bool">false</de-ice>
    <de-ice n="1" type="bool">false</de-ice>
   </visor>
   <window>
    <demist n="0" type="bool">false</demist>
    <demist n="1" type="bool">false</demist>
   </window>
   <windshield>
    <de-ice n="0" type="bool">false</de-ice>
    <de-ice n="1" type="bool">false</de-ice>
    <high n="0" type="bool">false</high>
    <high n="1" type="bool">false</high>
   </windshield>
   <wing>
    <alternate-selector type="int">0</alternate-selector>
    <main-selector type="int">0</main-selector>
   </wing>
  </anti-ice>

  <audio>
   <adf n="0">
    <knob n="0" type="double">0.0</knob>                      <!-- 3D instrument -->
    <knob n="1" type="double">0.0</knob>
    <knob n="2" type="double">0.0</knob>
   </adf>
   <adf n="1">
    <knob n="0" type="double">0.0</knob>                      <!-- 3D instrument -->
    <knob n="1" type="double">0.0</knob>
    <knob n="2" type="double">0.0</knob>
   </adf>
   <cancel type="bool">false</cancel>
   <comm n="0">
    <knob n="0" type="double">0.0</knob>                      <!-- 3D instrument -->
    <knob n="1" type="double">0.0</knob>
    <selected type="bool">true</selected>
    <standby type="bool">false</standby>
   </comm>
   <comm n="1">
    <knob n="0" type="double">0.0</knob>                      <!-- 3D instrument -->
    <knob n="1" type="double">0.0</knob>
    <selected type="bool">true</selected>
    <standby type="bool">false</standby>
   </comm>
   <crew>
    <captain>
     <adf n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>            <!-- knob disabled -->
     </adf>
     <adf n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <comm n="0">
      <volume type="double">0.6</volume>                      <!-- FG default -->
      <volume-push type="bool">false</volume-push>
     </comm>
     <comm n="1">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <nav n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
     <nav n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
    </captain>
    <copilot>
     <adf n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <adf n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <comm n="0">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <comm n="1">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <nav n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
     <nav n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
    </copilot>
    <engineer>
     <adf n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <adf n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <comm n="0">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <comm n="1">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <nav n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
     <nav n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
    </engineer>
    <observer>
     <adf n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <adf n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </adf>
     <comm n="0">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <comm n="1">
      <volume type="double">0.6</volume>
      <volume-push type="bool">false</volume-push>
     </comm>
     <nav n="0">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
     <nav n="1">
      <volume type="double">0.0</volume>
      <volume-push type="bool">false</volume-push>
     </nav>
    </observer>
   </crew>
  </audio>

  <autoflight>
   <altitude></altitude>
   <autopilot n="0">
    <engage type="bool">false</engage>
    <heading-select type="double">0.0</heading-select>
    <heading-true-select type="double">0.0</heading-true-select>
    <track-push type="bool">false</track-push>                <!-- magnetic heading -->
   </autopilot>
   <autopilot n="1">
    <engage type="bool">false</engage>
    <heading-select type="double">0.0</heading-select>
    <heading-true-select type="double">0.0</heading-true-select>
    <track-push type="bool">false</track-push>                <!-- magnetic heading -->
   </autopilot>
   <autothrottle n="0">
    <engage type="bool">false</engage>
   </autothrottle>
   <autothrottle n="1">
    <engage type="bool">false</engage>
   </autothrottle>
   <datum>
    <altitude type="double">0.0</altitude>
    <speed type="double">0.0</speed>
   </datum>
   <fg-waypoint type="bool">true</fg-waypoint>                <!-- default FG behaviour -->
   <flight-director n="0">
    <engage type="bool">false</engage>
   </flight-director>
   <flight-director n="1">
    <engage type="bool">false</engage>
   </flight-director>
   <heading></heading>
   <horizontal></horizontal>
   <real-nav type="bool">true</real-nav>
   <speed></speed>
   <speed2></speed2>
   <vertical></vertical>
   <vertical2></vertical2>
  </autoflight>

  <captain>
   <countdown type="bool">false</countdown>                   <!-- optional in practice -->
  </captain>

  <copilot>
   <activ type="bool">false</activ>
  </copilot>

  <crew>
   <captain-busy type="bool">true</captain-busy>              <!-- crew performs Captain checks -->
   <checklist type="bool">true</checklist>                    <!-- flight checklists triggers automatically -->
   <copilot-in-command type="bool">true</copilot-in-command>  <!-- only four engine flame out -->
   <ins-alignment type="bool">true</ins-alignment>            <!-- quick alignment of INS (not real) -->
   <landing-lights type="bool">true</landing-lights>          <!-- optional in checklist -->
   <night-lighting type="bool">true</night-lighting>
   <presets type="int">0</presets>                            <!-- taxi -->
   <radio type="bool">true</radio>                            <!-- nearest ADF and VOR -->
   <recall type="bool">false</recall>                         <!-- recall detection -->
   <startup type="bool">false</startup>                       <!-- inactiv on startup -->
   <steering>
    <pedal type="bool">false</pedal>
    <wheel alias="/fdm/jsbsim/systems/hydraulics/nws-engaged"/>
   </steering>
   <stop-engine23 type="bool">false</stop-engine23>           <!-- optional in checklist -->
   <timeout type="bool">true</timeout>                        <!-- of 10 seconds -->
   <timeout-s type="double">10.0</timeout-s>
  </crew>

  <doors>
   <flight-deck>
    <opened type="bool">false</opened>
    <normal type="bool">true</normal>
   </flight-deck>
   <engineer-deck>
    <out type="bool">false</out>
   </engineer-deck>
  </doors>

  <electric>
   <ac>
    <generator n="0" type="bool">true</generator>             <!-- generator to main bus -->
    <generator n="1" type="bool">true</generator>
    <generator n="2" type="bool">true</generator>
    <generator n="3" type="bool">true</generator>
    <ground type="bool">false</ground>                        <!-- ground service -->
    <gpb type="bool">false</gpb>                              <!-- ground power breaker -->
    <btb n="0" type="bool">true</btb>                         <!-- bus tie breaker -->
    <btb-guard n="0" type="bool">true</btb-guard>
    <btb n="1" type="bool">true</btb>
    <btb-guard n="1" type="bool">true</btb-guard>
    <btb n="2" type="bool">true</btb>
    <btb-guard n="2" type="bool">true</btb-guard>
    <btb n="3" type="bool">true</btb>
    <btb-guard n="3" type="bool">true</btb-guard>
    <ssb type="bool">true</ssb>                               <!-- split system breaker -->
    <essential n="0" type="bool">true</essential>             <!-- main to essential -->
    <essential n="1" type="bool">true</essential>
    <essential n="2" type="bool">true</essential>
    <essential n="3" type="bool">true</essential>
    <inverter-blue type="bool">true</inverter-blue>           <!-- inverter on -->
    <inverter-green type="bool">true</inverter-green>
    <relight n="0" type="bool">true</relight>                 <!-- relight to main bus -->
    <relight n="1" type="bool">true</relight>
    <relight n="2" type="bool">true</relight>
    <relight n="3" type="bool">true</relight>
    <standby type="bool">true</standby>                       <!-- standby to AC essential -->
    <voltmeter type="int">0</voltmeter>                       <!-- vertical -->
    <emergency>
     <asb type="bool">true</asb>                              <!-- auto shed breaker -->
     <essential n="0" type="bool">false</essential>           <!-- emergency bus to AC essential (manual) -->
     <essential n="1" type="bool">false</essential>
     <essential n="2" type="bool">false</essential>
     <essential n="3" type="bool">false</essential>
     <essential-auto n="0" type="bool">false</essential-auto> <!-- emergency bus to AC essential (automatic) -->
     <essential-auto n="1" type="bool">false</essential-auto>
     <essential-auto n="2" type="bool">false</essential-auto>
     <essential-auto n="3" type="bool">false</essential-auto>
     <generator>
      <arm type="bool">true</arm>                             <!-- emergency generator armed -->
      <arm-guard type="bool">true</arm-guard>
      <auto type="bool">true</auto>
      <auto-guard type="bool">true</auto-guard>
      <ground-bypass type="bool">true</ground-bypass>
      <selected type="bool">false</selected>                  <!-- generator to emergency bus -->
     </generator>
     <relight-selector type="int">0</relight-selector>        <!-- off -->
     <relight n="0" type="bool">false</relight>               <!-- emergency to relight -->
     <relight n="1" type="bool">false</relight>
     <relight n="2" type="bool">false</relight>
     <relight n="3" type="bool">false</relight>
     <standby type="bool">false</standby>                     <!-- standby to DC essential (RAT not deployed) -->
    </emergency>
   </ac>
   <dc>
    <inverter-blue type="bool">true</inverter-blue>           <!-- inverter powered -->
    <inverter-green type="bool">true</inverter-green>
    <master-bat n="0" alias="/controls/engines/engine[0]/master-bat"/>
    <master-bat n="1" type="bool">true</master-bat>           <!-- battery B -->
    <main-a type="bool">true</main-a>                         <!-- AC essential to DC main (TRU) -->
    <main-b type="bool">true</main-b>
    <essential-a type="bool">true</essential-a>               <!-- AC essential to DC essential (TRU) -->
    <essential-b type="bool">true</essential-b>
    <ems-a type="bool">true</ems-a>                           <!-- essential / main split -->
    <ems-b type="bool">true</ems-b>
    <voltmeter type="int">0</voltmeter>                       <!-- vertical -->
   </dc>
  </electric>

  <engineer>
   <activ type="bool">false</activ>
   <destination-ft type="int">0</destination-ft>
  </engineer>

  <engines>
   <engine n="0">
    <autoignition type="bool">true</autoignition>
    <autothrottle type="bool">true</autothrottle>
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <hp-valve type="bool">true</hp-valve>
    <intake-auto type="bool">true</intake-auto>
    <intake-main type="bool">true</intake-main>               <!-- main hydraulics -->
    <intake-selector type="int">0</intake-selector>           <!-- auto A -->
    <master-bat type="bool">true</master-bat>                 <!-- battery A (like the old C++ electrical system) -->
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <reheat type="bool">false</reheat>
    <reverser-angle-rad alias="/fdm/jsbsim/propulsion/engine[0]/reverser-angle-rad"/>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>                    <!-- differentiate autothrottle -->
   </engine>
   <engine n="1">
    <autoignition type="bool">true</autoignition>
    <autothrottle type="bool">true</autothrottle>
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <hp-valve type="bool">true</hp-valve>
    <intake-auto type="bool">true</intake-auto>
    <intake-main type="bool">true</intake-main>
    <intake-selector type="int">0</intake-selector>
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <reheat type="bool">false</reheat>
    <reverser-angle-rad alias="/fdm/jsbsim/propulsion/engine[1]/reverser-angle-rad"/>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>
   </engine>
   <engine n="2">
    <autoignition type="bool">true</autoignition>
    <autothrottle type="bool">true</autothrottle>
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <hp-valve type="bool">true</hp-valve>
    <intake-auto type="bool">true</intake-auto>
    <intake-main type="bool">true</intake-main>
    <intake-selector type="int">0</intake-selector>
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <reheat type="bool">false</reheat>
    <reverser-angle-rad alias="/fdm/jsbsim/propulsion/engine[2]/reverser-angle-rad"/>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>
   </engine>
   <engine n="3">
    <autoignition type="bool">true</autoignition>
    <autothrottle type="bool">true</autothrottle>
    <bug_pph>10000.0</bug_pph>
    <contingency type="bool">false</contingency>
    <csd type="bool">true</csd>
    <csd-guard type="bool">true</csd-guard>
    <hp-valve type="bool">true</hp-valve>
    <intake-auto type="bool">true</intake-auto>
    <intake-main type="bool">true</intake-main>
    <intake-selector type="int">0</intake-selector>
    <master-alt type="bool">true</master-alt>                 <!-- generator -->
    <n1-to-limit type="double">88.1</n1-to-limit>
    <n1-to-limiter type="bool">true</n1-to-limiter>           <!-- 88% -->
    <rating>takeoff</rating>
    <rating-flight>climb</rating-flight>
    <reheat type="bool">false</reheat>
    <reverser-angle-rad alias="/fdm/jsbsim/propulsion/engine[3]/reverser-angle-rad"/>
    <secondary-air-door type="bool">true</secondary-air-door>
    <throttle-manual>0.0</throttle-manual>
   </engine>

   <flight-reverse type="bool">false</flight-reverse>
   <ground-idle14 type="bool">false</ground-idle14>
   <ground-idle23 type="bool">false</ground-idle23>
   <schedule type="int">0</schedule>                          <!-- normal -->
   <schedule-auto type="bool">true</schedule-auto>
   <schedule-low type="bool">true</schedule-low>
  </engines>

  <environment>
   <als>
    <lights type="bool">true</lights>
   </als>
   <rain type="bool">false</rain>                             <!-- not visible from cockpit -->
   <smoke type="bool">true</smoke>
  </environment>

  <flight>
   <anti-stall>
    <system n="0" type="bool">false</system>
    <system n="1" type="bool">false</system>
   </anti-stall>
   <channel>
    <inner-blue type="bool">false</inner-blue>
    <inner-mechanical type="bool">false</inner-mechanical>
    <outer-blue type="bool">true</outer-blue>
    <outer-mechanical type="bool">false</outer-mechanical>
    <rudder-blue type="bool">true</rudder-blue>
    <rudder-mechanical type="bool">false</rudder-mechanical>
   </channel>
  </flight>

  <fuel>
   <jettison>
    <valve n="0">
     <flow type="bool">false</flow>
     <on type="bool">false</on>
     <shut type="bool">false</shut>
    </valve>
    <valve n="1">
     <flow type="bool">false</flow>
     <on type="bool">false</on>
     <shut type="bool">false</shut>
    </valve>
   </jettison>

   <pumps>
    <aft type="bool">false</aft>
    <aft-2D type="bool">false</aft-2D>                        <!-- 2D panel -->
    <auto-forward type="bool">false</auto-forward>
    <auto-guard type="bool">true</auto-guard>
    <auto-off type="bool">true</auto-off>
    <cross type="bool">false</cross>
    <dump type="bool">false</dump>
    <dump2 type="bool">false</dump2>
    <engine type="bool">false</engine>
    <forward type="bool">false</forward>
    <forward-guard type="bool">true</forward-guard>
    <forward-override type="bool">false</forward-override>
   </pumps>

   <reinit type="bool">true</reinit>                          <!-- restore fuel on reinit -->

   <tank n="0">
    <aft-trim type="bool">false</aft-trim>
    <aft-trim-disabled type="bool">true</aft-trim-disabled>
    <cross-feed-valve type="bool">false</cross-feed-valve>
    <heater type="bool">true</heater>
    <heater-auto type="bool">true</heater-auto>
    <inlet-standby type="bool">false</inlet-standby>
    <jettison type="bool">false</jettison>
    <lp-valve type="bool">true</lp-valve>                     <!-- engine on -->
    <lp-valve-motor type="bool">true</lp-valve-motor>         <!-- motor 1 -->
    <lp-valve-guard type="bool">true</lp-valve-guard>
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <pump n="2" type="bool">true</pump>
    <recirculation-valve type="bool">true</recirculation-valve>
   </tank>
   <tank n="1">
    <aft-trim type="bool">false</aft-trim>                    <!-- always -->
    <cross-feed-valve type="bool">false</cross-feed-valve>
    <heater type="bool">true</heater>
    <heater-auto type="bool">true</heater-auto>
    <inlet-standby type="bool">false</inlet-standby>
    <jettison type="bool">false</jettison>
    <lp-valve type="bool">true</lp-valve>
    <lp-valve-motor type="bool">true</lp-valve-motor>
    <lp-valve-guard type="bool">true</lp-valve-guard>
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <pump n="2" type="bool">true</pump>
    <recirculation-valve type="bool">true</recirculation-valve>
   </tank>
   <tank n="2">
    <aft-trim type="bool">false</aft-trim>                    <!-- always -->
    <cross-feed-valve type="bool">false</cross-feed-valve>
    <heater type="bool">true</heater>
    <heater-auto type="bool">true</heater-auto>
    <inlet-standby type="bool">false</inlet-standby>
    <jettison type="bool">false</jettison>
    <lp-valve type="bool">true</lp-valve>
    <lp-valve-motor type="bool">true</lp-valve-motor>
    <lp-valve-guard type="bool">true</lp-valve-guard>
    <pump n="0" type="bool">true</pump>
    <pump n="1" type="bool">true</pump>
    <pump n="2" type="bool">true</pump>
    <recirculation-valve type="bool">true</recirculation-valve>
   </tank>
