   <key n="357">
    <name>Up</name>
    <desc>Increase autopilot pitch (slow) or move floating view front (fast)</desc>
    <mod-ctrl>
     <binding>
      <command>nasal</command>
      <script>
       if( !globals.Concorde.seatsystem.movelengthexport(-0.1) ) {
           if( !globals.Concorde.autopilotsystem.datumapexport(0.1) ) {
               # default
           }
       }
      </script>
     </binding>
    </mod-ctrl>
   </key>

   <key n="359">
    <name>Down</name>
    <desc>Decrease autopilot pitch (slow) or move floating view aft (fast)</desc>
    <repeatable type="bool">true</repeatable>
    <mod-ctrl>
     <binding>
      <command>nasal</command>
      <script>
       if( !globals.Concorde.seatsystem.movelengthexport(0.1) ) {
           if( !globals.Concorde.autopilotsystem.datumapexport(-0.1) ) {
               # default
           }
       }
      </script>
     </binding>
    </mod-ctrl>
   </key>


   <key n="257">
    <name>F1</name>
    <desc>NAV1 +/- 1 MHz</desc>
    <repeatable type="bool">true</repeatable>

     <binding>
      <command>nasal</command>
      <script>
       f=getprop("/instrumentation/nav[1]/frequencies/selected-mhz");
       mod=getprop("/devices/status/keyboard/ctrl");

       if(mod==0) {
	 f=f+1;
         if (f>118){
           f=118;
         };      
       }

       if(mod==1) {
	 f=f-1;
         if (108>f){
           f=108;
         };      
       };

       setprop("/instrumentation/nav[1]/frequencies/selected-mhz",f);

      </script>
     </binding>

   </key>

